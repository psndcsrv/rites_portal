- options = { :complete => mce_init_string }
%div{ :id => dom_id_for(district), :class => "container_element" }
  = show_menu_for(district, options)

  %div{:id => dom_id_for(district, :item), :class => 'content'}
    = wrap_edit_link_around_content(district, options={:complete => mce_init_string}) do
      %ul
        %li= h(district.name)
        %li
          Description:
          = h(district.description)
